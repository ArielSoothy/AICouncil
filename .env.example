# AI Provider APIs
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Authentication
NEXTAUTH_SECRET=your_super_secret_nextauth_key_here
NEXTAUTH_URL=http://localhost:3000

# Optional: Redis for caching (leave empty for memory cache)
REDIS_URL=

# Rate Limiting (requests per minute per user)
RATE_LIMIT_RPM=10

# Database (for user management - optional for MVP)
DATABASE_URL=

# Alpaca Paper Trading API (REQUIRED for trading features)
# Get your keys from: https://alpaca.markets (Paper Trading section)
ALPACA_API_KEY=your_alpaca_paper_api_key_here
ALPACA_SECRET_KEY=your_alpaca_paper_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Environment
NODE_ENV=development
